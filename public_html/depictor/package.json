{
  "private": true,
  "version": "0.1.0",
  "main": "index.js",
  "engines": {
    "npm": ">=16.0.0 <20.0.0",
    "node" : ">=16.0.0 <17.0.0"
  },
  "scripts": {
    "build": "npm run build:js && npm run build:css",
    "build:css": "npm run build:sass",
    "build:js": "webpack -d",
    "build:sass": "sass scss/style.scss style.css",
    "dist": "npm run update-locales && npm run build:css && webpack -p",
    "postinstall": "npm run build",
    "update-locales": "node etc/update-locales.js",
    "watch": "chokidar '../common/*.scss' 'scss/*.scss' -c 'npm run build:css' & webpack -dw"
  },
  "dependencies": {
    "@babel/core": "^7.21.8",
    "chokidar-cli": "^2.1.0",
    "donot": "^0.4.1",
    "lodash-es": "^4.17.21",
    "loglevel": "^1.8.1",
    "regenerator-runtime": "^0.13.11",
    "sass": "^1.62.1",
    "valenski": "^1.5.0",
    "vue": "^2.7.14",
    "vue-i18n": "^8.28.2",
    "vuex": "^3.6.2"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-loader": "^8.3.0",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "css-loader": "^3.6.0",
    "http-server": "^0.13.0",
    "node-fetch": "^2.6.11",
    "vue-loader": "^15.10.1",
    "vue-template-compiler": "^2.7.14",
    "webpack": "^4.46.0",
    "webpack-cli": "^3.3.12"
  }
}
